[ENGINE]
Response File Version=1.0.0.0.0
# Não altere essa versão, é padrão do instalador.

[GENERIC]
ORACLE_HOME=/u01/app/oracle/middleware
# Diretório onde o WebLogic será instalado

INSTALL_TYPE=WebLogic Server
# Tipo de instalação. Outras opções incluem:
# "Coherence" (para instalar Oracle Coherence)
# "Complete with Examples" (para instalar tudo, incluindo exemplos)

[SYSTEM]
DECLINE_SECURITY_UPDATES=false
# Define se deseja receber atualizações de segurança automáticas (true/false)

SECURITY_UPDATES_VIA_MYORACLESUPPORT=false
# Se true, precisa informar os dados de login do Oracle Support
